        " textmode=1<wait>",
        "<enter><wait>"
      ],
      "boot_wait": "10s",
      "disk_size": 20480,
      "guest_os_type": "sles11-64",
      "headless": "{{ user `headless` }}",
      "http_directory": "http",
      "iso_checksum": "480b70d50cbb538382dc2b9325221e1b",
      "iso_checksum_type": "md5",
      "iso_url": "{{user `mirror`}}/SLES-11-SP3-DVD-x86_64-GM-DVD1.iso",
